    "space-after-colon": " "
    "space-after-combinator": " "
    "space-after-opening-brace": '''
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
    '''
    "space-before-closing-brace": '''
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
    '''
    "space-before-colon": ""
    "space-before-combinator": " "
    "space-before-opening-brace": " "
    "space-before-selector-delimiter": ""
    "space-between-declarations": '''
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
    '''
    "strip-spaces": true
    "unitless-zero": true
    "vendor-prefix-align": true
  editor:
    backUpBeforeSaving: true
    fontFamily: "SF Mono"
    fontSize: 12
    invisibles: {}
    lineHeight: 1.9
    showIndentGuide: true
    showInvisibles: true
    softWrapAtPreferredLineLength: true
  "exception-reporting":
    userId: "85e0c956-2bc8-b754-ca23-9f79d39a340b"
  "language-babel": {}
  linter: {}
  "linter-flow":
    executablePath: "./node_modules/.bin/flow"
  "linter-ui-default":
    decorateOnTreeView: "None"
    panelHeight: 72
  "markdown-preview":
    useGitHubStyle: true
  "nord-atom-syntax":
    accessibility:
      commentContrast: 10
  "nord-atom-ui":
    darkerFormFocusEffect: true
  "one-dark-ui":
    fontSize: 12
  "opened-files": {}
  "prettier-atom":
    formatOnSaveOptions: {}
    prettierOptions:
      jsxBracketSameLine: true
      singleQuote: true
      trailingComma: "all"
    silenceErrors: true
    useEslint: true
  "status-bar": {}
  tabs:
    usePreviewTabs: true
  "tree-view":
    focusOnReveal: false
    hideIgnoredNames: true
    squashDirectoryNames: true
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
