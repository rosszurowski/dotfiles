"*":
  "atom-package-deps":
    ignored: [
      "linter-ui-default"
    ]
  "autocomplete-modules":
    webpack: true
  "autocomplete-plus":
    scopeBlacklist: [
      ".source.gfm"
    ]
  autoprefixer: {}
  "color-picker":
    preferredFormat: "HEX"
  core:
    disabledPackages: [
      "metrics"
      "bookmarks"
      "background-tips"
    ]
    projectHome: "/User/rosszurowski/Sites/"
    telemetryConsent: "limited"
    themes: [
      "spacegray-dark-neue-ui"
      "nord-atom-syntax"
    ]
    titleBar: "custom"
  csscomb:
